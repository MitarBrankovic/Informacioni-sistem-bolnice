<Window x:Class="PrviProgram.Izgled.IzgledUpravnik.RadnoVremeIzvestaj"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PrviProgram.Izgled.IzgledUpravnik"
        mc:Ignorable="d"
        Title="RadnoVremeIzvestaj" Height="500" Width="550"
        KeyDown="Window_KeyDown">
    <Grid Background="SeaShell">
        <Label Content="Lekar:" HorizontalAlignment="Left" Height="25" Margin="135,141,0,0" VerticalAlignment="Top" Width="122"/>
        <Button x:Name="Konvertuj" Style="{StaticResource UpravnikButton}" Content="Konvertuj u PDF" HorizontalAlignment="Center" Height="47" Margin="0,256,0,0" VerticalAlignment="Top" Width="177" Click="Konvertuj_Click"/>
        <ComboBox x:Name="ComboLekar" HorizontalAlignment="Left" Margin="275,141,0,0" VerticalAlignment="Top" Width="120" SelectedIndex="0"/>
        <Label Content="Izvestaj radnog vremena izabranog lekara" HorizontalAlignment="Center" Margin="0,26,0,0" VerticalAlignment="Top" FontSize="24"/>
        <Button x:Name="Odustani" Style="{StaticResource UpravnikButton}" Content="Odustani" HorizontalAlignment="Center" Height="47" Margin="0,320,0,0" VerticalAlignment="Top" Width="177" Click="Odustani_Click"/>

    </Grid>
</Window>
